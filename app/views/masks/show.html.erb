<h1>Mask #<%= h(@mask.id) %> from Task <%= h(@task.id) %> <%= h(@task.name) %> </h1>

<%= form_tag task_mask_path(@task,@mask), :method => :delete do %>
 <%= link_to 'Edit',  edit_task_mask_path(@task, @mask) %>
 <%= link_to 'Task',  pipeline_task_path(@task.pipeline, @task) %>
 <%= link_to 'Masks',  task_masks_path(@task) %>
 <button type="submit" onclick="return confirm('Are you sure you want to delete?')" >Delete</button>
<% end %>

 <table border="0">
   <tr><td><label for="mask_name">Name:</label></td><td>
       <%= @mask.name %></td></tr>

   <tr><td><label for="mask_source">Source:</label></td><td>
       <%= @mask.source %></td></tr>

   <tr><td><label for="mask_value">Value:</label></td><td>
       <%= @mask.value %></td></tr>

   <tr><td><label for="mask_optionflags">Flags:</label></td><td>
       <%= @mask.optionflags %></td></tr>
 </table>
